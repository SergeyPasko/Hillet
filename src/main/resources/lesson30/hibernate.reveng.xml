<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="customers"
		match-catalog="classicmodels">
	</table-filter>
	<table-filter match-name="employees"
		match-catalog="classicmodels">
	</table-filter>
	<table-filter match-name="offices" match-catalog="classicmodels"></table-filter>
	<table-filter match-name="orderdetails"
		match-catalog="classicmodels">
	</table-filter>
	<table-filter match-name="orders" match-catalog="classicmodels"></table-filter>
	<table-filter match-name="payments" match-catalog="classicmodels"></table-filter>
	<table-filter match-name="productlines"
		match-catalog="classicmodels">
	</table-filter>
	<table-filter match-name="products" match-catalog="classicmodels"></table-filter>
</hibernate-reverse-engineering>